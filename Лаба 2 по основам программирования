#include <stdio.h>
#include <math.h>
int main(){
    double alpha;//в градусах
    double z1,z2;
    scanf("%lf",&alpha);
    z1=(cos(alpha*M_PI/180)+sin(alpha*M_PI/180))/(cos(alpha*M_PI/180)-sin(alpha*M_PI/180));
    z2=tan(2*alpha*M_PI/180)+1/cos(2*alpha*M_PI/180);//sec(alpha)=1/cos(alpha)
    printf("%.2lf %.2lf",z1,z2);
    return 0;
}
